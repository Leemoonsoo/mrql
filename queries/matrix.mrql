store "X"
  from select (random(1000)/1000.0,i,j)
         from i in 1...100, j in 1..200;

store "Y"
  from select (random(1000)/1000.0,i,j)
         from i in 1...200, j in 1..50;

// matrix multiplication

select (sum(select p.x*p.y from p in partition),i,j)
from (x,i,k) in source(binary,"X"),
     (y,k,j) in source(binary,"Y")
group by (i,j);
